<VBox alignment="CENTER" opacity="1.0" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.nyampass.cabin.app.Controller">
  <children>
    <HBox maxHeight="50.0" minHeight="60.0" prefHeight="60.0" prefWidth="-1.0" spacing="10.0">
      <children>
        <Button id="button" fx:id="startButton" alignment="CENTER" cancelButton="false" contentDisplay="RIGHT" defaultButton="false" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" mouseTransparent="false" onAction="#onStart" prefHeight="40.0" prefWidth="40.0" text="" textAlignment="LEFT" textOverrun="CLIP" HBox.hgrow="ALWAYS">
          <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" mouseTransparent="false" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@flag.png" />
              </image>
            </ImageView>
          </graphic>
          <HBox.margin>
            <Insets />
          </HBox.margin>
        </Button>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </HBox>
    <SplitPane dividerPositions="0.5" focusTraversable="true" prefHeight="160.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <items>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
          <tabs>
            <Tab text="UI Mosaic">
              <content>
                <TextArea fx:id="textArea" prefWidth="200.0" wrapText="true" />
              </content>
            </Tab>
          </tabs>
        </TabPane>
        <WebView fx:id="web" prefHeight="200.0" prefWidth="200.0" />
      </items>
    </SplitPane>
    <TextField prefHeight="150.0" prefWidth="800.0" />
  </children>
  <effect>
    <InnerShadow />
  </effect>
  <padding>
    <Insets />
  </padding>
</VBox>
